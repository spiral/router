{
    "name": "spiral/router",
    "type": "library",
    "description": "Routes, RESTFul, URI generation, Adaptive Patterns",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/router"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "spiral/core": "^2.7",
        "spiral/hmvc": "^2.7",
        "spiral/http": "^2.7",
        "cocur/slugify": "^3.2",
        "doctrine/inflector": "^1.4|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.4",
        "laminas/laminas-diactoros": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Router\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
